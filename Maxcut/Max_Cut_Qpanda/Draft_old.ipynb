    "from pyqpanda import *\n",
    "\n",
    "init(QMachineType.CPU)\n",
    "prog = QProg()\n",
    "q = qAlloc_many(2)\n",
    "c = cAlloc_many(2)\n",
    "prog.insert(H(q[0]))\n",
    "prog.insert(CNOT(q[0],q[1]))\n",
    "prog.insert(measure_all(q,c))\n",
    "result = run_with_configuration(prog, cbit_list = c, shots = 1000)\n",
    "print(result)\n",
    "finalize()"
   ]
